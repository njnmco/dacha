{
    "manifest_version": 3,
    "name" : "dacha (simple homepage)",
    "description" : "sets new tab page to a static page.",
    "version": "0.1.0",
    "permissions": ["storage", "activeTab", "scripting"],
    "action" : {},
    "chrome_url_overrides": {
        "newtab":"dacha.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}



